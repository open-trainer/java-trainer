# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# SQS Queue URLs
AWS_SQS_HEALTH_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/health-queue
AWS_SQS_NOTIFICATION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/notification-queue

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Application Configuration
SPRING_PROFILES_ACTIVE=local
LOG_LEVEL=INFO
APP_SCHEDULER_ENABLED=true
APP_SQS_POLL_INTERVAL=30000

# Database Configuration (for production, these would be managed by AWS Secrets Manager)
# DynamoDB table name is configured in the application code

# Monitoring (optional)
PROMETHEUS_ENABLED=true